  height: 2px;
}
/*----- GENERAL -*/
* {
  margin: 0px;
  padding: 0px;
}
body {
  font-family: 'Dosis', sans-serif;
  color: #000;
  margin: 0px;
  padding: 0px;
}
ul,
li {
  margin: 0px;
  padding: 0px;
  list-style: none;
}
/*-----------------------------------------------
MOBILE
Content of mobile menu
-----------------------------------------------*/
.mobile {
  padding: 30px;
}
/*-----------------------------------------------
LABEL
-----------------------------------------------*/
.mobile__label {
  border: solid 1px black;
  display: inline-block;
  overflow: hidden;
  padding: 12px 3px;
  width: 35px ;
  white-space: nowrap;
}
.mobile__label:hover {
  cursor: pointer;
  /*ANIMAION POSITION ON HOVER*/
  /*ANIMATION DELAY*/
}
.mobile__label:hover .label__line {
  margin-left: 0px;
}
.mobile__label:hover :nth-child(2) .label__line {
  transition-delay: 0.1s;
}
.mobile__label:hover :nth-child(3) .label__line {
  transition-delay: 0.2s;
}
/*LINES CONTENT*/
.mobile__label__content {
  font-size: 0;
  line-height: 0;
  height: 2px;
  margin-bottom: 4px;
}
.mobile__label__content:last-child {
  margin-bottom: 0px;
}
/*WHITE LINES*/
.label__line {
  background-color: #000;
  width: 24px;
  height: 2px;
  display: inline-block;
  margin-left: -33px;
  margin-right: 43px;

  /*LINES ANIMATION*/
  transition: margin-left 0.2s linear;
}
/*-----------------------------------------------
INPUT
-----------------------------------------------*/
.mobile__input {
  display: none;
}
/*-----------------------------------------------
NAV
-----------------------------------------------*/
.mobile__nav {
  max-height: 0;
  max-width: 0;
  overflow: hidden;
  transition: max-height 1s linear, max-width 0.8s linear;
}
.nav__item {
  white-space: nowrap;
}
.nav__item a {
  text-decoration: none;
  display: block;
  padding: 5px 7px;
  color: #000;
}
.nav__item a:after {
  display: block;
  width: 15px;
  height: 1px;
  background-color: #000;
  content: '';
}
.mobile__input:checked + .mobile__nav {
  max-width: 150px;
  max-height: 200px;
}

</style>
{% endblock %}#}

